#include <stdio.h>

#define		NO		 0
#define		YES		 1
#define		DELETE 2

int main(void)
{
	char line[1000];

	fgets(line, sizeof line, stdin); // use 'fgets' instead of 'gets'

	char *bufferPtr;
	char *startOfWord;
	char *endOfWord;
	int flag;
	int found;

	flag = NO;
	found = NO;
	bufferPtr = line - 1; // set this value because of the increment in the beginning of the cycle
	startOfWord = bufferPtr;
	endOfWord = bufferPtr;
	
	do
	{
		bufferPtr++;
		if ( *bufferPtr == ' ' || *bufferPtr == ',' || *bufferPtr == '.' 
		 || *bufferPtr == '\n' || *bufferPtr == '\0' ) // check for separator
		{
			if ( found == DELETE )
			{
				endOfWord = bufferPtr; // remember the end of the word
				bufferPtr = startOfWord; // set the buffer pointer on the beginning of the word
				while ( *bufferPtr++ = *endOfWord++ ); // word deletion

				bufferPtr = startOfWord - 1;
				found = NO;
			}
			else 
				found = ( *bufferPtr == ' ' && found || *bufferPtr == ',' ) ? YES : NO; // allow spaces after the comma
			
			flag = NO;
		}
		else 
		{
			if ( !flag  && found ) // write the beginning of the word
			{
				startOfWord = bufferPtr;
				found = DELETE;
			}
			
			flag = YES;
		}
	} while ( *bufferPtr );
	
	puts(line);
	getchar();
	return 0;
}
